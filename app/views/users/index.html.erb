<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Name</th>
      <th>Mother name</th>
      <th>Father name</th>
      <th>Dob</th>
      <th>Aadhar no</th>
      <th>Pan no</th>
      <th>Phone Number</th>
      <th>Adress</th>
      <th>Nationality</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.name %></td>
      <td><%= user.mother_name %></td>
      <td><%= user.father_name %></td>
      <td><%= user.dob %></td>
      <td><%= user.aadhar_no %></td>
      <td><%= user.pan_no %></td>
      <td><%= user.phno %></td>
      <td><%= user.adress %></td>
      <td><%= user.nationality %></td>
    </tr>
  <% end %>
</tbody>

<br>
